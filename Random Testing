import random

def generate_random_list(min_size=5, max_size=10, min_value=1, max_value=50):
    size = random.randint(min_size, max_size)
    

    random_list = [random.randint(min_value, max_value) for _ in range(size)]
    
    return random_list


list1 = generate_random_list()
list2 = generate_random_list()

print("Random List 1:", list1)
print("Sorted:", sorted(list1))
print()
print("Random List 2:", list2)
print("Sorted:", sorted(list2))
